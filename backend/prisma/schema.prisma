// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database/fullstack_curse.db"
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
  username String
  password String
}
